    {% for day in days %}
    <div class="grid grid--cols-1 gap--medium">
        <div class="grid grid--cols-3">
            <div class="col--span-2">
            <span class="value value--small text--black">{{ day.dow | upcase }} <span class="text--gray-3">{{ "20250217T000000" | date: "%Y-%m-%d %H:%M:%S" }}</span></span>
            </div>
            <div class="col--span-1 row--end">
                <span class="value value--small value--tnums text--gray-3">55¬∞/51¬∞</span>
                <span class="value value--small">‚õÖ</span>
            </div>
        </div>
    </div>
    {% for event in day.events %}
    <div>
        <h2>{{ event.Summary }}</h2>
        <p>{{ event.Description }}</p>
        <p><strong>Location:</strong> {{ event.Location }}</p>
        <p><strong>Start:</strong> {{ event.Start.Date }} {{ event.Start.Datetime }} ({{ event.Start.Timezone }})</p>
        <p><strong>End:</strong> {{ event.End.Date }} {{ event.End.Datetime }} ({{ event.End.Timezone }})</p>
        <p><strong>Status:</strong> {{ event.Status }}</p>
    </div>
    {% endfor %}
    {% endfor %}

</div>
